						"          CELDA_B as string,",
						"          LATITUD_LONGITUD_B as string,",
						"          FECHA as string,",
						"          DURACION as string",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     isolationLevel: 'READ_UNCOMMITTED',",
						"     format: 'table') ~> source1",
						"source1 select(mapColumn(",
						"          TIPO_SERVICIO,",
						"          TIPO_REGISTRO,",
						"          NUMERO_A,",
						"          IMEI_A,",
						"          RBS_UTILIZADA_A,",
						"          CELDA_A,",
						"          LATITUD_LONGITUD_A,",
						"          NUMERO_B,",
						"          IMEI_B,",
						"          RBS_UTILIZADA_B,",
						"          CELDA_B,",
						"          LATITUD_LONGITUD_B,",
						"          FECHA,",
						"          DURACION",
						"     ),",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true) ~> select1",
						"select1 derive(FECHA = trim(FECHA),",
						"          DURACION = toInteger(     iif(         isNull(DURACION) || DURACION == '',          '0',          toString(             (toInteger(split(DURACION, ':')[1]) * 3600) +              (toInteger(split(DURACION, ':')[2]) * 60) +              toInteger(split(DURACION, ':')[3])         )     ) )) ~> derivedColumn1",
						"derivedColumn1 cast(output(",
						"          FECHA as date 'dd-MM-yyyy',",
						"          DURACION as integer",
						"     ),",
						"     errors: true) ~> cast1",
						"cast1 sink(allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     input(",
						"          TIPO_SERVICIO as string,",
						"          TIPO_REGISTRO as string,",
						"          NUMERO_A as long,",
						"          IMEI_A as long,",
						"          RBS_UTILIZADA_A as string,",
						"          CELDA_A as integer,",
						"          LATITUD_LONGITUD_A as string,",
						"          NUMERO_B as long,",
						"          IMEI_B as long,",
						"          RBS_UTILIZADA_B as string,",
						"          CELDA_B as integer,",
						"          LATITUD_LONGITUD_B as string,",
						"          FECHA as date,",
						"          DURACION as integer",
						"     ),",
						"     deletable:false,",
						"     insertable:true,",
						"     updateable:false,",
						"     upsertable:false,",
						"     format: 'table',",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true,",
						"     errorHandlingOption: 'stopOnFirstError',",
						"     mapColumn(",
						"          TIPO_SERVICIO,",
						"          TIPO_REGISTRO,",
						"          NUMERO_A,",
						"          IMEI_A,",
						"          RBS_UTILIZADA_A,",
						"          CELDA_A,",
						"          LATITUD_LONGITUD_A,",
						"          NUMERO_B,",
						"          IMEI_B,",
						"          RBS_UTILIZADA_B,",
						"          CELDA_B,",
						"          LATITUD_LONGITUD_B,",
						"          FECHA,",
						"          DURACION",
						"     )) ~> sink1"
					]
				}
			},
