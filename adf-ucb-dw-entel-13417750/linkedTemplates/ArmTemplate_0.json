						"source(output(",
						"          SERVICIO as string,",
						"          REGISTRO as string,",
						"          NUMERO_A as string,",
						"          IMEI_A as string,",
						"          {RBS UTILIZADA_A} as string,",
						"          CELDA_A as string,",
						"          LATITUD_LONGITUD_A as string,",
						"          NUMERO_B as string,",
						"          IMEI_B as string,",
						"          RBS_UTILIZADA_B as string,",
						"          CELDA_B as string,",
						"          LATITUD_LONGITUD_B as string,",
						"          FECHA as string,",
						"          DURACION as string",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     ignoreNoFilesFound: false) ~> source1",
						"source1 sink(allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     input(",
						"          TIPO_SERVICIO as string,",
						"          TIPO_REGISTRO as string,",
						"          NUMERO_A as string,",
						"          IMEI_A as string,",
						"          RBS_UTILIZADA_A as string,",
						"          CELDA_A as string,",
						"          LATITUD_LONGITUD_A as string,",
						"          NUMERO_B as string,",
						"          IMEI_B as string,",
						"          RBS_UTILIZADA_B as string,",
						"          CELDA_B as string,",
						"          LATITUD_LONGITUD_B as string,",
						"          FECHA as string,",
						"          DURACION as string",
						"     ),",
						"     deletable:false,",
						"     insertable:true,",
						"     updateable:false,",
						"     upsertable:false,",
						"     format: 'table',",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true,",
						"     errorHandlingOption: 'stopOnFirstError',",
						"     mapColumn(",
						"          TIPO_SERVICIO = SERVICIO,",
						"          TIPO_REGISTRO = REGISTRO,",
						"          NUMERO_A,",
						"          IMEI_A,",
						"          RBS_UTILIZADA_A = {RBS UTILIZADA_A},",
						"          CELDA_A,",
						"          LATITUD_LONGITUD_A,",
						"          NUMERO_B,",
						"          IMEI_B,",
						"          RBS_UTILIZADA_B,",
						"          CELDA_B,",
						"          LATITUD_LONGITUD_B,",
						"          FECHA,",
						"          DURACION",
						"     )) ~> sink1"
					]
				}
