{
	"name": "df_silver_limpiar_datos",
	"properties": {
		"folder": {
			"name": "Silver"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_tabla_llamadas_entel",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_silver_llamadas_entel",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "cast1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          TIPO_SERVICIO as string,",
				"          TIPO_REGISTRO as string,",
				"          NUMERO_A as string,",
				"          IMEI_A as string,",
				"          RBS_UTILIZADA_A as string,",
				"          CELDA_A as string,",
				"          LATITUD_LONGITUD_A as string,",
				"          NUMERO_B as string,",
				"          IMEI_B as string,",
				"          RBS_UTILIZADA_B as string,",
				"          CELDA_B as string,",
				"          LATITUD_LONGITUD_B as string,",
				"          FECHA as string,",
				"          DURACION as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1",
				"source1 select(mapColumn(",
				"          TIPO_SERVICIO,",
				"          TIPO_REGISTRO,",
				"          NUMERO_A,",
				"          IMEI_A,",
				"          RBS_UTILIZADA_A,",
				"          CELDA_A,",
				"          LATITUD_LONGITUD_A,",
				"          NUMERO_B,",
				"          IMEI_B,",
				"          RBS_UTILIZADA_B,",
				"          CELDA_B,",
				"          LATITUD_LONGITUD_B,",
				"          FECHA,",
				"          DURACION",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 derive(FECHA = trim(FECHA)) ~> derivedColumn1",
				"derivedColumn1 cast(output(",
				"          FECHA as date 'dd-MM-yyyy',",
				"          DURACION as timestamp 'HH:mm:ss'",
				"     ),",
				"     errors: true) ~> cast1",
				"cast1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          TIPO_SERVICIO as string,",
				"          TIPO_REGISTRO as string,",
				"          NUMERO_A as long,",
				"          IMEI_A as long,",
				"          RBS_UTILIZADA_A as string,",
				"          CELDA_A as integer,",
				"          LATITUD_LONGITUD_A as string,",
				"          NUMERO_B as long,",
				"          IMEI_B as long,",
				"          RBS_UTILIZADA_B as string,",
				"          CELDA_B as integer,",
				"          LATITUD_LONGITUD_B as string,",
				"          FECHA as date,",
				"          DURACION as integer",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          TIPO_SERVICIO,",
				"          TIPO_REGISTRO,",
				"          NUMERO_A,",
				"          IMEI_A,",
				"          RBS_UTILIZADA_A,",
				"          CELDA_A,",
				"          LATITUD_LONGITUD_A,",
				"          NUMERO_B,",
				"          IMEI_B,",
				"          RBS_UTILIZADA_B,",
				"          CELDA_B,",
				"          LATITUD_LONGITUD_B,",
				"          FECHA,",
				"          DURACION",
				"     )) ~> sink1"
			]
		}
	}
}